{
  "name": "to-vuex-fire",
  "description": "A Vue.js project",
  "author": "Adam Romines <adam.romines@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "myIP=$(ifconfig  | grep 'inet addr:'| grep -v '127.0.0.1' | cut -d: -f2 | awk '{ print $1}'); NODE_ENV=development webpack-dev-server --inline --hot --host 0.0.0.0 --public $myIP:8080 --colors --open",
    "staging": "myIP=$(ifconfig  | grep 'inet addr:'| grep -v '127.0.0.1' | cut -d: -f2 | awk '{ print $1}'); NODE_ENV=staging webpack-dev-server --inline --hot --host 0.0.0.0 --public $myIP:8080",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "buildDev": "webpack --progress"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bulma": "^0.2.3",
    "firebase": "^3.7.2",
    "firebase-encode": "0.0.5",
    "lodash": "^4.17.4",
    "moment": "^2.15.2",
    "vue": "^2.2.6",
    "vue-router": "^2.4.0",
    "vuejs-datepicker": "^0.7.11",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-runtime": "^6.23.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^2.5.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.4.5"
  }
}
