{
  "name": "to-vuex-fire",
  "description": "A Vue.js project",
  "author": "Adam Romines <adam.romines@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "myIP=$(ifconfig | grep -A 1 'wlx20689d5bc6a2' | tail -1 | cut -d ':' -f 2 | cut -d ' ' -f 1); NODE_ENV=development webpack-dev-server --mode=development --inline --hot --host $myIP --port '8080' --public --colors --open",
    "staging": "myIP=$(ifconfig | grep -A 1 'wlx20689d5bc6a2' | tail -1 | cut -d ':' -f 2 | cut -d ' ' -f 1); NODE_ENV=staging webpack-dev-server --inline --hot --host $myIP --port '8081' --public",
    "build": "cross-env NODE_ENV=production webpack --mode=production --progress --hide-modules",
    "buildStage": "cross-env FIREBASE=staging webpack --progress --hide-modules",
    "buildDev": "webpack --progress",
    "printIp": "myIP=$(ifconfig | grep -A 1 'wlx20689d5bc6a2' | tail -1 | cut -d ':' -f 2 | cut -d ' ' -f 1) && echo $myIP"
  },
  "dependencies": {
    "bulma": "^0.2.3",
    "firebase": "^4.5.2",
    "firebase-encode": "0.0.5",
    "lodash": "^4.17.4",
    "moment": "^2.21.0",
    "vue": "^2.5.16",
    "vue-cleave-component": "^1.0.2",
    "vue-clipboard2": "^0.1.0",
    "vue-router": "^3.0.1",
    "vuejs-datepicker": "^0.7.11",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.4.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "vue-loader": "^14.2.2",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.4.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
